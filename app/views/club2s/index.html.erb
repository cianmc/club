<h1>Listing club2s</h1>

<table>
<style>
	table {
		border-collapse: collapse;
		width: 100%;
	}

	table, td, th {
		border: 1px solid black;
		text-align: left;
	}
</style>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @club2s.each do |club2| %>
      <tr>
        <td><%= club2.name %></td>
        <td><%= club2.address %></td>
        <td><%= link_to 'Show', club2 %></td>
        <td><%= link_to 'Edit', edit_club2_path(club2) %></td>
        <td><%= link_to 'Destroy', club2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Club2', new_club2_path %>
